include mixins

doctype
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0,",
		"minimum-scale=1.0")
		meta(http-equiv="X-UA-Compatible" content="ie=edge")
		title Book Api
	body
		+modalWindow('sign-in-modal', 'sign-in-close-btn', 'Sign in')
			include sign-in
		+modalWindow('log-in-modal', 'log-in-close-btn', 'Log in')
			include log-in
		include header
		main
			block content
		.snackbar#snackbar



